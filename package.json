{
  "name": "bookmaker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint src test docs assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"{docs,assets}/**/*.{js,css,html,json}\" \"*.{md,json,mjs,html}\"",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.30.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}
